{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "target": "ES6",
        "module": "CommonJS",
        "strict": true,
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "noEmit": false,
        "esModuleInterop": true
    },
    "include": ["src"]
}
